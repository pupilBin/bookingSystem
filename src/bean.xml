<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans.xsd">
    <bean id="arrangement" class="beans.arrangement" scope="prototype"></bean>
    <bean id="freeTime" class="beans.freeTime" scope="prototype"></bean>
    <bean id="user" class="beans.user" scope="prototype"></bean>
    <bean id="changeData" class="service.changeData" scope="singleton"></bean>
</beans>